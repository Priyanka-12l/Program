ublic class SwapNumbers {

    public static void main(String[] args) {

        float first = 1.20f, second = 2.45f;

        // Display values before swap
        System.out.println("--Before swap--");
        System.out.println("First number = " + first);  // Added missing semicolon
        System.out.println("Second number = " + second); // Added missing semicolon

        // Swapping logic using a temporary variable
        float temporary = first; // Store the value of 'first' in temporary

        first = second; // Assign the value of 'second' to 'first'
        second = temporary; // Assign the value of 'temporary' (which holds the original 'first') to 'second'

        // Display values after swap
        System.out.println("--After swap--");
        System.out.println("First number = " + first);
        System.out.println("Second number = " + second);
